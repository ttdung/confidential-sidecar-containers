{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "containerGroupName": {
      "defaultValue": "aciSKRSidecarTest",
      "type": "String",
      "metadata": {
        "description": "Secure Key Release Sidecar Example"
      }
    }
  },
  "resources": [
    {
      "type": "Microsoft.ContainerInstance/containerGroups",
      "apiVersion": "2023-05-01",
      "name": "[parameters('containerGroupName')]",
      "location": "[resourceGroup().location]",
      "identity": {
        "type": "UserAssigned",
        "userAssignedIdentities": {
          "/subscriptions/63bf8da0-6f25-4075-8fdf-8ec3655d3ede/resourcegroups/container_group/providers/Microsoft.ManagedIdentity/userAssignedIdentities/manage_id_name": {}
        }
      },
      "properties": {
        "containers": [
          {
            "name": "skr-sidecar-container",
            "properties": {
              "command": [
                "/skr.sh"
              ],
              "image": "mcr.microsoft.com/aci/skr:2.12",
              "resources": {
                "requests": {
                  "cpu": 1,
                  "memoryInGB": 2
                }
              },
              "ports": [
                {
                  "port": 8080
                }
              ]
            }
          },
          {
            "name": "test-sum",
            "properties": {
              "environmentVariables": [
                {
                  "name": "SkrClientMAAEndpoint",
                  "value": "sharedneu.neu.attest.azure.net"
                },
                {
                  "name": "SkrClientAKVEndpoint",
                  "value": "testvaultkhtn.vault.azure.net"
                },
                {
                  "name": "SkrClientKID",
                  "value": "mykeyv7"
                }
              ],
              "image": "ttdungacr.azurecr.io/cacidemo:3",
              "resources": {
                "requests": {
                  "cpu": 1,
                  "memoryInGB": 1
                }
              }
            }
          }
        ],
        "osType": "Linux",
        "ipAddress": {
          "type": "Public",
          "ports": [
            {
              "protocol": "tcp",
              "port": 8080
            }
          ]
        },
        "sku": "confidential",
        "confidentialComputeProperties": {
          "ccePolicy": ""
        },
        "imageRegistryCredentials": [
          {
            "server": "ttdungacr.azurecr.io",
            "username": "ttdungacr",
            "password": "Gx8bFhcd7KBcOl28wvngS0WWuRPs1s008IIC2z1vTZ+ACRDcFbWf"
          }
        ]
      }
    }
  ],
  "outputs": {
    "containerIPv4Address": {
      "type": "String",
      "value": "[reference(resourceId('Microsoft.ContainerInstance/containerGroups/', parameters('containerGroupName'))).ipAddress.ip]"
    }
  }
}